# LinkedIn Clone Environment Variables
# Copy this file to .env and update the values

# MongoDB Database Connection
MONGODB_URI=mongodb://localhost:27017/linkedin-clone

# JWT Secret - You can use any text you want!
# Examples:
# JWT_SECRET=mylinkedinproject2024
# JWT_SECRET=simple-secret-key
# JWT_SECRET=linkedinclone123
JWT_SECRET=your-custom-secret-here

# Server Port
PORT=5000